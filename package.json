{
	"name": "vscodefileheader",
	"displayName": "VSCodeFileHeader",
	"description": "header comment",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:python",
		"onCommand:extension.fileheader"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration":{
			"type": "object",
			"title": "File Header Configuration",
			"properties": {
				"fileheader.author": {
					"type": "string",
					"default": "Your name",
					"description": "Your name"
				},
				"fileheader.dateformat": {
					"type": "string",
					"default": "YYYY-mm-dd HH:MM:SS",
					"description": "DateTime Format"
				}
			}
		},
		"commands": [
			{
				"command": "extension.fileheader",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"art-template": "^4.13.2"
	}
}
